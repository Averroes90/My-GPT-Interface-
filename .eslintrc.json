{
    "extends": [
      "eslint:recommended",
      "plugin:vue/essential",
      "prettier",
      "prettier/vue"
    ],
    "plugins": [
      "prettier",
      "import",
      "promise",
      "security"
    ],
    "env": {
      "es2021": true,
      "browser": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": "error"
    },
    "parserOptions": {
      "ecmaVersion": 2023,
      "sourceType": "module"
    },
    "ignorePatterns": ["static/dist/", "node_modules/"]
  }